  "bugs": {
    "url": "https://github.com/CoolDogee/cap-that-pic/issues"
  },
  "homepage": "https://github.com/CoolDogee/cap-that-pic#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.4"
  }
}
