  "text": [
    "The",
    "silver",
    "hour",
    "drops\u2014",
    "a spider",
    "on the mirror.",
    "*",
    "Silver",
    "the hour",
    "like drops",
    "of a spider\u2019s",
    "mirror.",
    "*",
    "The silver drops,",
    "the spider\u2019s hour,",
    "the mirror . . ."
  ],
  "title": "A Start",
  "year": ""
}